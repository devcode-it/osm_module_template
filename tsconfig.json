{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "strictPropertyInitialization": false,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "baseUrl": ".",
    "paths": {
      "@osm/*": ["./vendor/devcode-it/openstamanager/resources/ts/*"]
    }
  },
  "include": [
    "resources/ts/*"
  ]
}
