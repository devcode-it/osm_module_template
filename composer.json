{
  "name": "openstamanager/example.module",
  "description": "Modulo di esempio per OpenSTAManager",
  "type": "openstamanager-module",
  "license": "MIT",
  "keywords": [
    "openstamanager",
    "gestionale",
    "modulo",
    "esempio"
  ],
  "authors": [
    {
      "name": "Maicol Battistini",
      "email": "maicolbattistini@live.it"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "devcode-it/openstamanager": "3.x-dev"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Openstamanager\\TipiAttivita\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "ExampleModuleServiceProvider"
      ]
    },
    "osm-modules": {
      "attivita.tipi": {
        "icon": "format-list-bulleted-type",
        "parent-module": "openstamanager/example"
      }
    }
  },
  "scripts": {
    "post-package-update": "gulp copyGlobalscp  ./resources/js/globals.d.ts"
  },
  "scripts-descriptions": {
    "post-package-update": "Copia il file globals.d.ts dal package di OpenSTAManager"
  }
}
